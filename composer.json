{
    "name": "paradisesecurity/guzzle-bundle",
    "type": "symfony-bundle",
    "description": "Integrates the Guzzle HTTP Client into Symfony.",
    "homepage": "https://paradisesecurity.work",
    "license": "MIT",
    "authors": [
        {
            "name": "Paradise Security",
            "email": "joe@paradisesecurity.work",
            "homepage": "https://paradisesecurity.work",
            "role": "Owner"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/framework-bundle": "^6.4 || ^7.0",
        "symfony/expression-language": "^6.4 || ^7.0",
        "symfony/stopwatch": "^6.4 || ^7.0",
        "guzzlehttp/psr7": "^1.9.1 || ^2.5.1",
        "psr/log": "^2.0 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^6.4 || ^7.0",
        "symfony/twig-bundle": "^6.4 || ^7.0",
        "symfony/var-dumper": "^6.4 || ^7.0",
        "symfony/yaml": "^6.4 || ^7.0"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "suggest": {
        "namshi/cuzzle": "Outputs Curl command on profiler's page for debugging purposes"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "ParadiseSecurity\\Bundle\\GuzzleBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "ParadiseSecurity\\Bundle\\GuzzleBundle\\Test\\": "tests/Test/" }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../*"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
